//User function Template for C++
class Solution
{
    public:
    string reverseWord(string str)
    {
        // Your code goes here
        int n=str.length();
        int i=0,j=n-1;
        while(i<=j){
            char a=str[i];
            str[i]=str[j];
            str[j]=a;
            i++;
            j--;
        }
        return str;
    }
};
