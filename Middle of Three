//User function template for C++

class Solution{
  public:
    int middle(int A, int B, int C){
        //code here//Position this line where user code will be pasted.
        if(A>B){
            if(A<C){
                return A;
            }
             else if(C>B){
                return C;
            }
            else{
                return B;
            }
        }
        else{
            if(B<C){
                return B;
            }
             else if(C>A){
                return C;
            }
            else{
                return A;
            }
            
        }
    }
};
